{
  "name": "ng-backend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ng-backend/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["barrel", "^build"],
      "options": {
        "outputPath": "dist/packages/ng-backend",
        "main": "packages/ng-backend/src/index.ts",
        "tsConfig": "packages/ng-backend/tsconfig.lib.json"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/ng-backend/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "dependsOn": ["barrel", "^test"],
      "options": {
        "jestConfig": "packages/ng-backend/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "barrel": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "./packages/ng-backend",
        "command": "barrelsby --config ./barrelsby.json --directory ./src/lib"
      }
    },
    "docs": {
      "executor": "nx:run-commands",
      "dependsOn": ["build", "^docs"],
      "options": {
        "cwd": "./packages/ng-backend",
        "command": "typedoc"
      }
    }
  },
  "tags": []
}
