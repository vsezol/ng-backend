{
  "name": "ng-backend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ng-backend/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["^build"],
      "options": {
        "outputPath": "dist/packages/ng-backend",
        "main": "packages/ng-backend/src/index.ts",
        "tsConfig": "packages/ng-backend/tsconfig.lib.json",
        "assets": ["packages/ng-backend/*.md", "packages/ng-backend/.npmrc"]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "dist/packages/ng-backend",
        "commands": ["NPM_TOKEN={args.token} npm publish"]
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/ng-backend/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/ng-backend/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "docs": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "./packages/ng-backend",
        "command": "typedoc"
      }
    }
  },
  "tags": [
    "ng-backend",
    "interceptors",
    "angular",
    "typescript",
    "decorators",
    "mocks",
    "http"
  ]
}
