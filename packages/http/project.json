{
  "name": "http",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/http/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["^build"],
      "options": {
        "outputPath": "dist/packages/http",
        "main": "packages/http/src/index.ts",
        "tsConfig": "packages/http/tsconfig.lib.json",
        "assets": ["packages/http/*.md", "packages/http/.npmrc"]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "dist/packages/http",
        "commands": ["npm publish"]
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/http/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/http/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "docs": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "./packages/http",
        "command": "typedoc"
      }
    }
  },
  "tags": [
    "ng-backend",
    "interceptors",
    "angular",
    "typescript",
    "decorators",
    "mocks",
    "http"
  ]
}
